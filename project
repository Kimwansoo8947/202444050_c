#include<stdio.h>

int main() {
	int sum = 0;
	int num;

	printf("양수를 입력하시오: ");
	scanf("%d", &num);

	while (num > 0) {
		sum += num;
		printf("지금까지 누적 된 합: %d\n", sum);
		printf("양수를 입력하시오: ");
		scanf("%d", &num);
	}
	if (num <= 0) {
		printf("종료됩니다.\n");
		
	}
	return 0;
}
// 다음 문제중 결과 값을 예측하시오
